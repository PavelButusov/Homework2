def bubble(n):
    for i in range(len(n)):
        for j in range(len(n) - 1, i, -1):
            if n[j] < n[j-1]:
                n[j], n[j-1] = n[j-1], n[j]

    return n

if __name__ == '__main__':
    lst = [5,71,4,64,3,34,8]
    res = bubble(lst)
    print(res)
    
# сложность O(n * n)
# По факту самый больший элемент перемещаем по списку, отправляя его все выше и выше
# последовательно сравнивая один элемент с другим
# это довольно простой и тривиальный для понимания метод сортировки
